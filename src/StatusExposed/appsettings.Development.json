{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabasePath": "MainDatabase.sqlite",
  "LegalNotePath": "LegalNoticeTest.md",
  "ApiInfoPath": "ApiInfoTest.md",
  "AutomaticUpdates": false,
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/*",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "15m",
        "Limit": 100
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "12h",
        "Limit": 1000
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "7d",
        "Limit": 10000
      }
    ]
  },
  "ClientRateLimitPolicies": {
    "ClientRules": [
      {
        "ClientId": "test",
        "Rules": [
          {
            "Endpoint": "*:/api/*",
            "Period": "1m",
            "Limit": 100
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "15m",
            "Limit": 1000
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "12h",
            "Limit": 10000
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "7d",
            "Limit": 100000
          }
        ]
      }
    ]
  }
}